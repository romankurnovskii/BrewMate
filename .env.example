
APPLE_ID=
APPLE_APP_SPECIFIC_PASSWORD=
APPLE_TEAM_ID=

# Apple Code Signing Configuration
# Copy this file to .env and fill in your values
# Never commit .env to version control!

# Code signing identity (from: security find-identity -p codesigning -v)
# Example: "3rd Party Mac Developer Application: Your Name (TEAM_ID)"
APPLE_SIGNING_IDENTITY=3rd Party Mac Developer Application: Your Name (TEAM_ID)

APPLE_INSTALLER_IDENTITY=

# Provisioning profile path (relative to project root)
APPLE_PROVISIONING_PROFILE=.credentials/BrewMate_Provision_Distribute_Mac_App_Store.provisionprofile
